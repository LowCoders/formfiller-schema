{
  "name": "formfiller-schema",
  "version": "5.1.10",
  "description": "Shared schema definitions for FormFiller application with unified expression system, consolidated lookup.dataSource, and Stepper component support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && mkdir -p dist/schemas && cp src/schemas/*.json dist/schemas/ && npm run transform:schema && npm run generate:monaco",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate:types": "ts-node scripts/generate-types.ts",
    "generate:docs": "ts-node scripts/generate-docs.ts",
    "generate:monaco": "npx ts-node scripts/generate-monaco-types.ts",
    "transform:schema": "npx ts-node scripts/transform-schema.ts",
    "compose:schemas": "ts-node scripts/compose-schemas.ts",
    "migrate": "ts-node scripts/migrate-config.ts",
    "validate:all": "ts-node scripts/validate-all.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "csfixer": "prettier --write src && eslint src --ext .ts --fix",
    "csfixer:check": "prettier --check src && eslint src --ext .ts",
    "sync": "./sync-dependents.sh",
    "sync:auto": "AUTO_RESTART=true ./sync-dependents.sh",
    "sync:dry": "DRY_RUN=true ./sync-dependents.sh",
    "update-dependents": "./update-dependents.sh",
    "distribute": "./update-dependents.sh",
    "distribute:auto": "AUTO_RESTART=true ./update-dependents.sh",
    "distribute:dry": "DRY_RUN=true ./update-dependents.sh"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "form",
    "schema",
    "validation",
    "devextreme"
  ],
  "author": "Vivi and Sam <lowcoders@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "json-schema-to-typescript": "^13.1.1",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
